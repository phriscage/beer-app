<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="GenerateAccessTokenCodeJWT">
    <DisplayName>GenerateAccessTokenCodeJWT</DisplayName>
    <Operation>GenerateAccessToken</Operation>
    <ExpiresIn ref="oauth.code.expires_in">3600000</ExpiresIn>
    <SupportedGrantTypes>
        <!-- This part is very important: most real OAuth 2.0 apps will want to use other
         grant types. In this case it is important to NOT include the "client_credentials"
         type because it allows a client to get access to a token with no user authentication -->
        <GrantType>authorization_code</GrantType>
    </SupportedGrantTypes>
    <StoreToken>true</StoreToken>
    <ExternalAccessToken>jwt_signed</ExternalAccessToken>
    <ExternalAuthorization>false</ExternalAuthorization>
    <!--<ResponseType>request.formparam.response_type</ResponseType>-->
    <ClientId>request.formparam.client_id</ClientId>
    <RedirectUri>request.formparam.redirect_uri</RedirectUri>
    <Code>request.formparam.code</Code>
    <GenerateResponse enabled="true"/>
</OAuthV2>