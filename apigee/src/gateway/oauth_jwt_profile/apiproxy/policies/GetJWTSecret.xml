<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="GetJWTSecret" mapIdentifier="jwt">
    <DisplayName>GetJWTSecret</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
    <Get assignTo="private.secret" index="1">
        <Key>
            <Parameter>secret</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.secret_kid" index="1">
        <Key>
            <Parameter>secret_kid</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.privateKey" index="1">
        <Key>
            <Parameter>private_key</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.certificate1_kid" index="1">
        <Key>
            <Parameter>certificate1_kid</Parameter>
        </Key>
    </Get>
    <!--<Get assignTo="private.client_secret" index="1">-->
    <!--    <Key>-->
    <!--        <Parameter>client_secret</Parameter>-->
    <!--    </Key>-->
    <!--</Get>-->
    <Scope>environment</Scope>
</KeyValueMapOperations>